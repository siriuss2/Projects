@model List<MemberListViewModel>

<div class="row">
    @foreach (var member in Model)
    {
        <div class="col-md-4 mb-3">
            <div class="card border-success">
                <div class="card-body text-success">
                    <h5 class="card-title">Name: @member.Name</h5>
                    <h5 class="card-title">Phone number: @member.PhoneNumber</h5>
                    <h5 class="card-title">Membership Status: @member.MembershipStatus</h5>
                    <button type="button" class="btn"><a asp-action="Details" asp-route-id="@member.Id">Details</a></button>
                </div>
            </div>
        </div>
    }
</div>
